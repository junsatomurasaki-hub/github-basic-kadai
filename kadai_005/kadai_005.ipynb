        "# 台形の面積の計算結果を出力するプログラム\n",
        "\n",
        "top_side = 10\n",
        "under_side = 20\n",
        "high = 5\n",
        "\n",
        "# 計算結果をいったん変数 area に代入\n",
        "area = (top_side + under_side) * high / 2\n",
        "\n",
        "# area を出力\n",
        "print(f\"{area}cm²\")"
      ],
